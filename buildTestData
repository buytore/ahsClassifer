import re
def get_numbers_from_filename(filename):
    regex = r"(\d+)"    
    matches = re.findall(regex, filename)
    return int(matches[len(matches)-1])

path = os.getcwd()                                      #Get the current directory
print "Path: ", path
baseDir = next(os.walk(path))[1]                 #Get all the files in current directory

print "The directory names please: ", baseDir[0]
learnDir = os.path.join(path, baseDir[0])
testDir =  os.path.join(path, baseDir[1])
categoryNames = next(os.walk(learnDir))[1]

for category in categoryNames:
    learnPath = os.path.join(learnDir, category)
    filesInPath = next(os.walk(learnPath))[2]
    for file in filesInPath:
        filename, file_extension = os.path.splitext(file)
        fileNumber = get_numbers_from_filename(file)
        if fileNumber in testDict[category]:
            testPath = os.path.join(testDir, category)
            origFile = os.path.join(learnPath, file)
            testFile = os.path.join(testPath, file)
            os.rename(origFile, testFile)
print "All done"
